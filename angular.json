{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "slidecontent-angular-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "pt",
        "locales": {
          "en": {
            "translation": "src/locale/messages.en.xlf"
          },
          "es": {
            "translation": "src/locale/messages.es.xlf"
          },
          "ru": {
            "translation": "src/locale/messages.ru.xlf"
          },
          "zh": {
            "translation": "src/locale/messages.zh.xlf"
          },
          "fr": {
            "translation": "src/locale/messages.fr.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/slidecontent-angular"
            },
            "index": "src/index.html",
            "localize": true,
            "i18nMissingTranslation": "error",
            "polyfills": [
              "src/polyfills.ts",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "pt": {
              "localize": [
                "pt"
              ]
            },
            "en": {
              "localize": [
                "en"
              ]
            },
            "es": {
              "localize": [
                "es"
              ]
            },
            "ru": {
              "localize": [
                "ru"
              ]
            },
            "zh": {
              "localize": [
                "zh"
              ]
            },
            "fr": {
              "localize": [
                "fr"
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "slidecontent-angular-app:build:production"
            },
            "pt": {
              "buildTarget": "slidecontent-angular-app:build:development,pt"
            },
            "en": {
              "buildTarget": "slidecontent-angular-app:build:development,en"
            },
            "es": {
              "buildTarget": "slidecontent-angular-app:build:development,es"
            },
            "ru": {
              "buildTarget": "slidecontent-angular-app:build:development,ru"
            },
            "zh": {
              "buildTarget": "slidecontent-angular-app:build:development,zh"
            },
            "fr": {
              "buildTarget": "slidecontent-angular-app:build:development,fr"
            },
            "development": {
              "buildTarget": "slidecontent-angular-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "slidecontent-angular-app:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": [
              "src/polyfills.ts",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "slidecontent-angular": {
      "projectType": "library",
      "root": "projects/slidecontent-angular",
      "sourceRoot": "projects/slidecontent-angular/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/slidecontent-angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/slidecontent-angular/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/slidecontent-angular/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/slidecontent-angular/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "6b10cc30-af0c-43f4-8469-d77dcc36ec01"
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
